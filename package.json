    "react-native-swipeout": "^2.3.3",
    "react-native-swiper": "^1.5.13",
    "react-native-tab-view": "^1.0.2",
    "react-native-vector-icons": "^4.6.0",
    "react-native-webview": "5.9.1",
    "react-navigation": "^2.6.2",
    "react-redux": "^5.0.7",
