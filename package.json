{
  "name": "iSalonManager",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "run-i": "react-native run-ios --simulator='iPhone 11 Pro Max'",
    "run-a": "react-native run-android",
    "build:android:dev": "react-native bundle --platform android --dev true --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:android:prod": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "push-staging-a": "appcenter codepush release-react --development -a isalon-dev/isalon-provider-app-android -d Staging",
    "push-staging-i": "appcenter codepush release-react --development -a isalon-dev/isalon-provider-app-ios -d Staging",
    "push-prod-a": "appcenter codepush release-react -a isalon-dev/isalon-provider-app-android -d Production",
    "push-prod-i": "appcenter codepush release-react -a isalon-dev/isalon-provider-app-ios -d Production"
  },
  "rnpm": {
    "assets": [
      "src/assets/fonts/"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "appcenter": "3.0.3",
    "appcenter-analytics": "3.0.3",
    "appcenter-crashes": "3.0.3",
    "axios": "^0.18.0",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
